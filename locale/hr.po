# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/main.py, line: 245
#, python-brace-format
msgid ""
"Warning: Using mitigations=off will make your machine less secure! For more information see: <a "
"href='{link}'>{link}</a>"
msgstr ""
"Upozorenje: Korištenje mitigations=off će učiniti vaš uređaj manje sigurnim! Za više informacija "
"pogledajte: <a href='{link}'>{link}</a>"
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/main.py, line: 251
msgid "noWatchdog"
msgstr "noWatchdog"
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/main.py, line: 252
msgid ""
"Disables the hardware watchdog and TSC clocksource systems, maintaining high performance but "
"removing automatic protections against system crashes."
msgstr ""
"Onemogućuje hardverski nadzor i TSC izvor vremena, održavajući visoke performanse, ali uklanjajući "
"automatske zaštite od rušenja sustava."
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/main.py, line: 258
msgid "IRQ Forced Threading"
msgstr "IRQ prisilno nitičenje"
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/main.py, line: 259
msgid "Warning: If you do not know what this is and/or are not sure, DO NOT enable this feature."
msgstr "Upozorenje: Ako ne znate što je ovo i/ili niste sigurni, NE aktivirajte ovu funkciju."
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/main.py, line: 266
msgid "Multi/Hyper Threading OFF"
msgstr "Multi/Hyper Threading ISKLJUČENO"
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/main.py, line: 267
#, python-brace-format
msgid ""
"Simultaneous Multithreading (SMT) or hyperthreading can cause spikes in DSP load at higher DSP "
"loads.\n"
"It can improve single-task performance, but it can worsen multitasking and other programs, so use "
"it wisely.\n"
"<a href='{link}'>{link}</a>"
msgstr ""
"Simultanozno višestruko nitovanje (SMT) ili hiperthreading mogu uzrokovati skokove u opterećenju "
"DSP-a pri višim opterećenjima DSP-a. Može poboljšati performanse jedne zadaće, ali može pogoršati "
"multitasking i druge programe, stoga ga koristite mudro. <a href='{link}'>{link}</a>"
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/main.py, line: 273
msgid "User in Audio Group"
msgstr "Korisnik u Audio Grupi"
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/main.py, line: 274
msgid ""
"It is generally good practice to have an audio group, and add any users that should be allowed to "
"perform audio tasks to this group."
msgstr ""
"Općenito je dobra praksa imati audio grupu i dodati sve korisnike koji bi trebali imati dopuštenje "
"za izvođenje audio zadataka u ovu grupu."
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/audio_system_tweaks_page.py, line: 216
msgid "Memlock and rtprio"
msgstr "Memlock i rtprio"
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/audio_system_tweaks_page.py, line: 217
msgid "Set memlock to unlimited and rtprio to 90."
msgstr "Postavite memlock na neograničeno i rtprio na 90."
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/audio_system_tweaks_page.py, line: 223
msgid "Disabled Wifi"
msgstr "Onemogućen Wifi"
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/main.py, line: 281
msgid ""
"NetworkManager keeps scanning for new wireless networks in the background and this might cause "
"xruns. The best option is to not use a wireless network in a low-latency real-time audio "
"environment."
msgstr ""
"NetworkManager neprekidno pretražuje nove bežične mreže u pozadini, što može uzrokovati xrune. "
"Najbolja opcija je ne koristiti bežičnu mrežu u okruženju s niskom latencijom za real-time audio."
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/audio_system_tweaks_page.py, line: 230
msgid "Disabled Bluetooth"
msgstr "Onemogućen Bluetooth"
# 
# File: pipewire-proaudio-config/usr/share/pipewire-proaudio-config/audio_system_tweaks_page.py, line: 231
msgid ""
"Bluetooth keeps scanning for new devices in the background and this might cause xruns. The best "
"option is to not use a bluetooth in a low-latency real-time audio environment."
msgstr ""
"Bluetooth neprekidno pretražuje nove uređaje u pozadini, što može uzrokovati xruns. Najbolja opcija "
"je ne koristiti Bluetooth u okruženju s niskom latencijom u stvarnom vremenu za audio."
